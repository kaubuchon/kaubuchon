{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      },
      "WriteTo": [
        { "Name": "Console" }
      ]
    }
  },
  "AllowedHosts": "*",
  "RecipeFolder": "recipes"
}
